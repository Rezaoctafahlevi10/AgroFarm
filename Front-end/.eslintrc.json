{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style" : 0,
        "no-alert": 0,
        "no-underscore-dangle": 0,
        "import/no-extraneous-dependencies": 0,
        "no-undef": 0,
        "no-unused-vars": 0,
        "prefer-destructuring" : 0,
        "no-console" : 0,
        "quote-props": 0
    }
}
